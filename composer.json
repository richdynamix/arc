{
    "name": "richdynamix/arc",
    "description": "Production ready docker based development environment for your Laravel project.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Richardson",
            "email": "steven@richdynamix.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support" : "6.*|7.*|8.*",
        "illuminate/console": "6.*|7.*|8.*"
    },
    "autoload": {
        "psr-4": {
            "Richdynamix\\Arc\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Richdynamix\\Arc\\ArcServiceProvider"
            ],
            "aliases": {
                "Arc": "Richdynamix\\Arc\\ArcFacade"
            }
        }
    }
}
